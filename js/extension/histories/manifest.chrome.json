{
    "manifest_version": 2,
    "name": "Histories",
    "version": "1.0",
    "description": "",
    "homepage_url": "https://github.com/gjp0609/Scripts/tree/master/js/extension/histories",
    "icons": {
        "48": "icons/history.png",
        "96": "icons/history.png"
    },
    "browser_action": {
        "default_icon": "icons/history.png",
        "default_title": "__MSG_extension_name__"
    },
    "background": {
        "page": "background.html"
    },
    "options_ui": {
        "browser_style": false,
        "open_in_tab": true,
        "page": "options.html"
    },
    "permissions": ["history", "storage", "unlimitedStorage", "chrome://favicon/"]
}
